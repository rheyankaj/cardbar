});

jQuery('#pme_cardbar_info_container').on('mouseleave',
function() {
jQuery('#profilevideo').empty();
var info = document.getElementById('pme_cardbar_info_container');
if (info.classList) {
info.classList.remove('visible');
} else {
info.className = info.className.replace(new RegExp('(^|\\b)' + 'visible'.split(' ').join('|') + '(\\b|$)', 'gi'), ' ');
}
});

// close info window
close_button.addEventListener('click', function(el) {
var info = document.getElementById('pme_cardbar_info_container');
jQuery('#profilevideo').empty();
if (info.classList) {
info.classList.remove('visible');
} else {
info.className = info.className.replace(new RegExp('(^|\\b)' + 'visible'.split(' ').join('|') + '(\\b|$)', 'gi'), ' ');
}
});
});
