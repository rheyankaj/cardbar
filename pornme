function pme_load_cardbar() {
pornme.load_jquery(function() {
var cardbar = document.getElementById('pme_cardbar_bar');

// calculate amateur count for available height
var height = window.innerHeight - parseInt(getComputedStyle(cardbar)['top']);
var amateur_count = parseInt(height / 65);
// fill with amateurs
for (var i = 0; i < amateur_count; i++) {
var ama_data = amateurs[i];
var amateur = document.createElement("a");
amateur.setAttribute("href", "#");
amateur_image = document.createElement("div");
amateur_img = document.createElement("img");
amateur_img.setAttribute("src", "http://cache.pornme.com/images/profile/" + ama_data['UID'] + "_290_218_18.jpg");
amateur_image.appendChild(amateur_img);
amateur.appendChild(amateur_image);

var countrymap = {
'Germany': 'Deutschland',
'Austria': 'Österreich',
'Switzerland': 'Schweitz',
'Spain': 'Spanien',
'Poland': 'Polen'
};

jQuery(amateur).data('username', ama_data['username']);
jQuery(amateur).data('UID', ama_data['UID']);
jQuery(amateur).data('age', ama_data['age']);
jQuery(amateur).data('zip', ama_data['zip'].substring(0, 2) + 'xxx');
jQuery(amateur).data('vkey', ama_data['vkey']);
jQuery(amateur).data('numvids', ama_data['numvids']);
jQuery(amateur).data('numpics', ama_data['numpics']);
jQuery(amateur).data('weight', ama_data['weight']);
jQuery(amateur).data('height', ama_data['height']);
jQuery(amateur).data('zodiac', ama_data['zodiac']);
jQuery(amateur).data('country', ((typeof countrymap[ama_data['country']] == 'undefined') ? ama_data['country'] : countrymap[ama_data['country']]));
jQuery(amateur).data('aboutme', ama_data['aboutme']);
jQuery(amateur).data('appearance', ama_data['appearance']);

cardbar.appendChild(amateur);
};
